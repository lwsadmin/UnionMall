        <script src="/lib/admin/js/plugins/gritter/jquery.gritter.min.js"></script>
        <script src="/lib/admin/js/plugins/sparkline/jquery.sparkline.min.js"></script>*@

    @*<script src="/lib/admin/js/demo/sparkline-demo.js"></script>*@

</body>
</html>

