@using System.Globalization
@using System.Threading
@using System.Threading.Tasks
@using Abp.Timing
@using UnionMall.Web.Views.Shared.Components.TopBar
@using Abp.Web.Security.AntiForgery
@using UnionMall.Web.Views.Shared.Components.SideBarNav
@inject IAbpAntiForgeryManager AbpAntiForgeryManager
@{
    AbpAntiForgeryManager.SetCookie(Context);
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@L("UnionMall")</title>
    <link href="/lib/admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/lib/admin/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/lib/admin/css/plugins/toastr/toastr.min.css" rel="stylesheet">


    <link href="/lib/admin/css/style.css" rel="stylesheet">
    <script src="/lib/admin/js/jquery-3.1.1.min.js"></script>

</head>
<body>
    <div id="wrapper">
        @await Component.InvokeAsync(typeof(SideBarNavViewComponent), new { FirstMenu = ViewBag.FirstMenu, SecondMenu = ViewBag.SecondMenu })
        <div id="page-wrapper" class="gray-bg dashbard-1">
            @await Component.InvokeAsync(typeof(TopBarViewComponent))

            @RenderBody()
        </div>

    </div>

    <!-- Mainly scripts -->
    <script src="/lib/admin/js/bootstrap.min.js"></script>
    <script src="/lib/admin/js/inspinia.js"></script>
    <script src="/lib/admin/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/lib/admin/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
